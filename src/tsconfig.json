{
	"compilerOptions": {
		"target": "ES5",
		"module": "system",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [ "es2015", "dom" ],
		"removeComments": false,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true
	}
}

